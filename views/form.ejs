<form id="Form" action="/form" method="post">
    <table width="400">
        <tr>
            <th>Name</th>
            <td><input type="text" name="name" id="name" size="10" maxlength="10"></td>
        </tr>
        <tr>
            <th>Phone</th>
            <td><input type="tel" name="phone" id="phone" size="20" maxlength="20"></td>
        </tr>
        <tr>
            <th>Id</th>
            <td><input type="number" name="Id" id="Id" size="10" maxlength="10"></td>
        </tr>
        <tr>
            <td><input type="submit"></td>
        </tr>
    </table>
</form>